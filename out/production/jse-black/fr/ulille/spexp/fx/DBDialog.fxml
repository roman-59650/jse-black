<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox prefHeight="255.0" prefWidth="210.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.ulille.spexp.fx.DBDialog">
   <children>
      <ScrollPane hbarPolicy="NEVER" prefHeight="318.0" prefWidth="210.0" vbarPolicy="NEVER">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="186.0" prefWidth="210.0">
               <children>
                  <ListView fx:id="listview" prefHeight="200.0" prefWidth="203.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               </children>
            </AnchorPane>
        </content>
      </ScrollPane>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="6.0">
         <children>
            <Button fx:id="selectdb" mnemonicParsing="false" onAction="#onSelectDatabaseClick" prefWidth="128.0" text="Select Database">
               <font>
                  <Font size="12.0" />
               </font>
            </Button>
            <Button fx:id="createdb" mnemonicParsing="false" onAction="#onCreateDbClick" prefWidth="128.0" text="Create Database...">
               <font>
                  <Font size="12.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</VBox>
